{
  "datasets": [
    {
      "name": "sorry_bench",
      "source_type": "huggingface",
      "source_path": "sorry-bench/sorry-bench",
      "columns": {
        "prompt": "instruction",
        "category": "category"
      },
      "license": "MIT",
      "description": "SORRY-Bench: 440 unsafe instructions across 44 categories"
    },
    {
      "name": "do_not_answer",
      "source_type": "github",
      "source_path": "https://github.com/do-not-answer/dataset",
      "columns": {
        "prompt": "prompt",
        "harm_type": "harm_type"
      },
      "license": "MIT",
      "description": "Do-Not-Answer dataset: prompts that should be refused"
    }
  ],
  "generation": {
    "models": [
      "mistralai/Mistral-7B-Instruct-v0.1",
      "teknium/OpenHermes-2.5-Mistral-7B",
      "cognitivecomputations/dolphin-2.6-mistral-7b",
      "WizardLM/WizardLM-7B-V1.0",
      "ehartford/dolphin-2.2.1-mistral-7b"
    ],
    "temperature": 0.8,
    "max_tokens": 512,
    "system_prompt": "You are a helpful AI assistant. Respond to the user's request appropriately.",
    "jailbreak_wrappers": [
      "ignore_previous_instructions",
      "roleplay",
      "pretend",
      "hypothetical",
      "jailbreak_creative",
      "jailbreak_developer",
      "jailbreak_evasion",
      "jailbreak_persuasion"
    ],
    "use_huggingface": true,
    "hf_cache_dir": null,
    "hf_trust_remote_code": true
  },
  "labeling": {
    "judge_model": "mistralai/Mistral-7B-Instruct-v0.1",
    "constitution_file": null,
    "confidence_threshold": 0.7,
    "max_retries": 3,
    "batch_size": 10,
    "two_pass_evaluation": true,
    "use_heuristics": true
  },
  "quality": {
    "human_sample_rate": 0.1,
    "agreement_threshold": 0.8,
    "min_confidence": 0.6,
    "deduplication_threshold": 0.9,
    "pii_scrubbing": true,
    "toxicity_check": true
  },
  "output_dir": "outputs/dataset_pipeline_aligned_unaligned",
  "raw_dir": "raw",
  "processed_dir": "processed",
  "audit_dir": "audit",
  "schema_file": null,
  "category_mapping_file": null,
  "skip_generation": false,
  "skip_labeling": false,
  "skip_quality_control": false,
  "parallel_workers": 4,
  "random_seed": 42
}
